{
  "manifest_version": 3,
  "name": "Color Picker",
  "description": "Pick any color from your screen",
  "version": "1.0.0",
  "author": "soyluisarrieta",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "color-picker-icon.png"
    }
  },
  "icons": {
    "128": "color-picker-icon.png"
  }
}
